<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LuckyWinner.Views.SideMenuView">
	<ListView ItemsSource="{Binding Path=MenuOptions}">
		<ListView.ItemTemplate>
			<DataTemplate>
				<ContentView>
					<ContentView.Content>
						<Label Text="{Binding}" />
					</ContentView.Content>
				</ContentView>
			</DataTemplate>
		</ListView.ItemTemplate>
	</ListView>
</ContentView>